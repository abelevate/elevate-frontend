{
  "manifest_version": 3,
  "name": "Autofill Form Extension",
  "version": "1.0",
  "description": "Automatically fills forms with user data.",
  "permissions": ["storage", "activeTab",  "scripting"],
  "host_permissions": ["https://elevate-user-backend.onrender.com/"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"],
    "run_at": "document_end"
  }]
  
}
